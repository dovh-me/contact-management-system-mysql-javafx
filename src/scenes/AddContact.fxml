<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="700.0" prefWidth="900.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AddContactController">
   <children>
      <Button fx:id="okBtn" layoutX="431.0" layoutY="421.0" mnemonicParsing="false" onAction="#addContact" text="OK" />
      <Label layoutX="271.0" layoutY="315.0" text="Enter Name">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Label layoutX="271.0" layoutY="350.0" text="Enter Number">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="contactNameTf" layoutX="451.0" layoutY="317.0" prefHeight="31.0" prefWidth="216.0" />
      <TextField fx:id="contactNumberTf" layoutX="451.0" layoutY="352.0" prefHeight="31.0" prefWidth="216.0" promptText="10 digit number &lt; [(2^31)-1]" />
      <Label layoutX="302.0" layoutY="126.0" text="Add New Contact">
         <font>
            <Font name="System Bold Italic" size="36.0" />
         </font>
      </Label>
      <Button fx:id="backBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#backToContactList" text="Back" />
   </children>
</AnchorPane>
